  "dangerous_patterns": {
    "command_injection": [
      "\\s*;\\s*\\w+",
      "\\s*\\|\\s*\\w+",
      "\\s*&&\\s*\\w+",
      "\\s*\\|\\|\\s*\\w+",
      "`[^`]+`",
      "\\$\\([^)]+\\)",
      ">\\s*/",
      "<\\s*/",
      ">>\\s*/",
      "2>\\s*/",
      "&>\\s*/"
    ],
    "path_traversal": [
      "\\.\\./",
      "\\.\\.\\\\",
      "/etc/",
      "/proc/",
      "/sys/",
      "/dev/",
      "/var/log/",
      "/var/run/",
      "/tmp/\\.\\./"
    ],
    "code_execution": [
      "eval\\s*\\(",
      "exec\\s*\\(",
      "subprocess\\.",
      "os\\.system",
      "os\\.popen",
      "os\\.spawn",
      "__import__",
      "compile\\s*\\(",
      "globals\\s*\\(\\)",
      "locals\\s*\\(\\)",
      "getattr\\s*\\(",
      "setattr\\s*\\(",
      "hasattr\\s*\\("
    ],
    "privilege_escalation": [
      "sudo\\s+",
      "su\\s+",
      "passwd\\s+",
      "useradd\\s+",
      "usermod\\s+",
      "chown\\s+root",
      "chmod\\s+[467]77",
      "systemctl\\s+",
      "service\\s+"
    ],
    "network_attacks": [
      "nc\\s+-[el]",
      "netcat\\s+-[el]",
      "wget\\s+http",
      "curl\\s+http",
      "telnet\\s+",
      "ssh\\s+.*@",
      "scp\\s+.*:",
      "rsync\\s+.*:",
      "socat\\s+"
    ],
    "data_exfiltration": [
      "base64\\s+.*\\|",
      "xxd\\s+.*\\|",
      "od\\s+.*\\|",
      "hexdump\\s+.*\\|",
      "tar\\s+.*\\|\\s*nc",
      "dd\\s+.*\\|\\s*nc",
      "cat\\s+/etc/passwd",
      "cat\\s+/etc/shadow",
      "cat\\s+.*\\.key",
      "cat\\s+.*id_rsa"
    ]
  },
  "resource_limits": {
